<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="#00ffffff"/>
                    <stroke android:width="2dp" android:color="@color/dark_green"/>
                    <corners android:radius="20dp"/>
                </shape>
            </item>
            <item android:state_focused="true">
                <shape android:shape="rectangle">
                    <solid android:color="#00ffffff"/>
                    <stroke android:width="2dp" android:color="@color/dark_green"/>
                    <corners android:radius="20dp"/>
                </shape>
            </item>
            <item android:state_selected="true">
                <shape android:shape="rectangle">
                    <solid android:color="#00ffffff"/>
                    <stroke android:width="2dp" android:color="@color/dark_green"/>
                    <corners android:radius="20dp"/>
                </shape>
            </item>
            <item android:state_enabled="false">
                <shape android:shape="rectangle">
                    <solid android:color="#00ffffff"/>
                    <stroke android:width="1dp" android:color="?attr/colorControlNormal"/>
                    <corners android:radius="20dp"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#00ffffff"/>
                    <stroke android:width="1dp" android:color="@color/t5"/>
                    <corners android:radius="20dp"/>
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
